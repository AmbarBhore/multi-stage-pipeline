pipeline {
     agent any {
	 IMAGE_NAME = "Rmm-agent"
	 IMAGE_TAG = "${BUILD_NUMBER}"
	 DOCKER_REPO = "ambarbhore1234"
	 K8S_DEPLOYMENT = "k8/s"
	 KUBECONFIG = credentials('k8s-jenkins-kubeconfig')
     }
     stages {
	stage('checkout code') {
	    steps {
		echo 'checking out the source code'
		checkout scm
	    }
	}
      }
}
